# # criar o nosso aplicativo
# # criar a função build

from kivy.app import App
from kivy.lang import Builder
import requests

GUI = Builder.load_file("tela.kv")

class MeuAplicativo(App):
    def build(self):
        return GUI
       
    def on_start(self):
        self.root.ids["lixo1"].text = "segunda-feira}"
        self.root.ids["lixo2"].text = "quarta-feira}"
        self.root.ids["livo3"].text = "sexta-feira}"
        
        return 
        
MeuAplicativo().run()
